{
  "private": true,
  "name": "energyweb-marketplace-web-app",
  "productName": "EnergyWeb Marketplace Web-based App",
  "description": "Energy Web Foundation Marketplace Web-based App",
  "version": "0.0.1",
  "author": "Energy Web X <hello@energyweb.org>",
  "expressPort": 3003,
  "imports": {
    "@main/*": "../main/*",
    "@ewf/*": "../renderer/*"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck": "tsc --noEmit -p ./tsconfig.json --composite false",
    "start": "vite preview",
    "dev": "npm run validate-env-vars && vite dev",
    "preview": "npm run validate-env-vars && npm run preview:watch",
    "validate-env-vars": "tsx validate-browser-envs.ts",
    "build": "npm run validate-env-vars && npm run typecheck && vite build --config vite.config.ts",
    "preview:watch": "concurrently \"vite preview -l silent --config vite.config.ts\" \"vite build --watch --config vite.config.ts\""
  },
